<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.user.catcher.monitor_script</string>

    <key>ProgramArguments</key>
    <array>
        <string>/opt/homebrew/bin/python</string>
        <string>~/.catcher/daemon.py</string>
    </array>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <true/>

    <key>StandardErrorPath</key>
    <string>~/.catcher/com.user.monitor_script.err</string>

    <key>StandardOutPath</key>
    <string>~/.catcher/com.user.monitor_script.out</string>
</dict>
</plist>
